{
  "name": "todoapp2",
  "version": "1.0.0",
  "description": "ragastodoapp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Raga TODO app\"",    
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Git-Raga/electron_projects.git"
  },
  "author": "Raga",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Git-Raga/electron_projects/issues"
  },
  "homepage": "https://github.com/Git-Raga/electron_projects#readme",
  "dependencies": {
    "electron": "^2.0.4"
  },
  "devDependencies": {
    "electron-packager": "^12.1.0"
  }
}
